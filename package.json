{
  "name": "gatsby-source-newt",
  "version": "2.0.25",
  "description": "Gatsby source plugin for building websites using Newt as a data source",
  "main": "gatsby-node.js",
  "files": [
    "dist",
    "gatsby-node.js"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier --check ./src",
    "fix:eslint": "prettier --fix ./src",
    "fix:prettier": "prettier --write ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Newt-Inc/gatsby-source-newt.git"
  },
  "author": "Newt, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Newt-Inc/gatsby-source-newt/issues"
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "newt-client-js": "^3.3.7"
  },
  "devDependencies": {
    "@types/node": "22.13.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "gatsby": "^5.14.0",
    "prettier": "^2.5.1",
    "typescript": "^5.5.3"
  }
}
